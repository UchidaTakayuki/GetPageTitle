{
  "manifest_version": 2,
  "name": "Get Page Title",
  "version": "1.0.0",
  "description": "Gets the current page title.",
  "permissions": ["contextMenus", "activeTab", "clipboardWrite"],
  "background": {
    "scripts": ["background.js"],
    "persistant": false
  },
   "icons": {
     "16": "icons/get_page_title_icon_16.png",
     "48": "icons/get_page_title_icon_48.png",
     "128": "icons/get_page_title_icon_128.png"
  }
}